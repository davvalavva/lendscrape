{
  "int": {
    "JStype": "number",
    "description": "no decimals, only whole numbers (integers)"
  },
  "timestamp": {
    "JStype": "string",
    "regex": "^[0-9]{4}-[0-9]{2}-[0-9]{2}\\s[0-9]{2}:[0-9]{2}:[0-9]{2}\\sUTC((\\+|-)\\d{1,2})?$",
    "description": "Example: 2018-01-13 12:38:11 UTC+1"
  }
}
